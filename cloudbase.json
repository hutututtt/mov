{
  "version": "1.0.0",
  "framework": "express",
  "source": {
    "type": "package",
    "package": {
      "name": "movie-streaming-platform",
      "version": "1.0.0"
    }
  },
  "functions": [
    {
      "name": "app",
      "runtime": "Nodejs12.16",
      "entry": "server.js",
      "timeout": 30,
      "memory": 128,
      "envVariables": {
        "NODE_ENV": "production"
      }
    }
  ],
  "triggers": [
    {
      "name": "http",
      "type": "http",
      "config": {
        "methods": ["GET", "POST", "PUT", "DELETE", "HEAD", "OPTIONS"],
        "path": "/"
      }
    }
  ]
}
